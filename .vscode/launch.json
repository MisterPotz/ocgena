    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch via NPM",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
            ],
            "program": "${workspaceFolder}\\build\\js\\packages\\ocgena\\kotlin\\ocgena.js",
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\build\\js\\packages\\ocgena\\kotlin\\ocgena.js",
            "outFiles": [
                "${workspaceFolder}/build/js/packages/ocgena/kotlin/**/*.js"
            ],
            "args": [
                "--enable-source-maps"
            ]
        }
    ]
}