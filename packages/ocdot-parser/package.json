{
  "name": "ocdot-parser",
  "version": "1.0.0",
  "description": "Sandbox for testing out functionality related to javascript, which afterwards may be converted to Kotlin Multiplatform Javascript flavor.",
  "main": "./lib/ast.js",
  "scripts": {
    "build": "npm run generate-peggy-grammar && npm run compile",
    "compile": "echo $(pwd) && tsc --project ./tsconfig.json",
    "test": "jest",
    "generate-peggy-grammar": "peggy --plugin ts-pegjs --allowed-start-rules ocdot,ocnet,subgraph,node,edge,attributes,attribute,cluster_statements,RootExpression -o ./src/ocdot.peggy.ts ./grammar/ocdot.peggy && prettier --write ./src/ocdot.peggy.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/shelljs": "^0.8.11",
    "jest": "^29.5.0",
    "peggy": "^3.0.2",
    "prettier": "2.8.6",
    "ts-jest": "^29.1.0",
    "ts-pegjs": "^3.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "shelljs": "^0.8.5",
    "ts-graphviz": "^1.5.5"
  },
  "types": "./lib/ast.d.ts",
  "typings": "./lib/ast.d.ts"
}
